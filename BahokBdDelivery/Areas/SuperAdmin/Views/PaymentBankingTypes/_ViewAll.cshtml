@model IEnumerable<BahokBdDelivery.Models.PaymentBankingType>


@if (Model != null)
{
    <table class="table table-responsive table-striped">
        <tbody>
            @foreach (var item in Model)
            {
                <tr>

                    <td style="width:1050px;">
                        @Html.DisplayFor(modelItem => item.BankingMethodName)
                    </td>
                    <td> <a onclick="showInPopup('@Url.Action("AddOrEdit","PaymentBankingTypes",new { id=item.Id},Context.Request.Scheme)','Update PaymentBankingTypes' )" class="btn btn-sm btn-info text-white"><i class="fas fa-random"></i></a></td>
                </tr>
            }
        </tbody>
    </table>
}    