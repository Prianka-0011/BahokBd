@model BahokBdDelivery.Models.PaymentBankingType
@{
    ViewData["Title"] = "AddOrEdit";
    Layout = null;
}

<div class="row">
    <div class="col-12 col-lg-10 ">
        <div class="card mb-4">
            <div class="card-header">
               Create Type
            </div>
            <div class="card-body">
                <form asp-action="AddOrEdit"asp-route-id="@Model.Id" onsubmit="return jQueryAjaxPost(this);" autocomplete="off">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="form-group">
                        <input type="hidden" asp-for="Id" />
                        <label asp-for="BankingMethodName" class="control-label"></label>
                        <input asp-for="BankingMethodName" class="form-control" />
                        <span asp-validation-for="BankingMethodName" class="text-danger"></span>
                    </div>
                 
                    <div class="form-group">
                        <input type="submit" value="Submit" class="btn btn-primary" />
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>

