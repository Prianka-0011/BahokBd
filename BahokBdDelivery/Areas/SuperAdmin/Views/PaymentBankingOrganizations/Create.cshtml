@model BahokBdDelivery.Models.PaymentBankingOrganization

@{
    ViewData["Title"] = "Create";
}

<div class="row">
    <div class="col-12 col-lg-6 offset-lg-3">
        <div class="card mb-4">
            <div class="card-header bg-info">
                <h2>Create</h2>
            </div>
            <div class="card-body">
                <form asp-action="Create">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="form-group">
                        <label asp-for="OrganizationName" class="control-label"></label>
                        <input asp-for="OrganizationName" class="form-control" />
                        <span asp-validation-for="OrganizationName" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="PaymentBankingTypeId" class="control-label"></label>
                        <select asp-for="PaymentBankingTypeId" class="form-control" asp-items="ViewBag.PaymentBankingTypeId"></select>
                    </div>
                    <div class="form-group">
                        <label asp-for="Status"></label>
                        <input asp-for="Status" type="checkbox" />
                        <span asp-validation-for="Status"></span>
                    </div>
                    <div class="form-group">
                        <input type="submit" value="Create" class="btn btn-primary" />
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
 

